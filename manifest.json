{
    "manifest_version": 3,
    "name": "YouTubeTimer",
    "version": "1.0",
    "permissions": [
        "tabs",
        "scripting"
      ],
    "background":{
        "service_worker": "background.js"
    },
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
        {
        "matches": ["*://youtube.com/watch/*"],
        "js": ["Content.js"],
        "css": []
        }
    ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": "Logo.png"
    }
}